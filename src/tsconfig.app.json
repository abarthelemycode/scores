    "paths": {
       "@env/*": ["environments/*"],
       "@core/*": ["app/core/*"],
       "@stations/*": ["app/stations/*"],
       "@shared/*": ["app/shared/*"],
    },
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
